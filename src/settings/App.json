{
  "props": {
    "App#default": {
      "saga": "handleApps",
      "left": [
        { "component": "AppSwitcher" },
        { "component": "SelectionList", "componentId": "components" },
        { "component": "SelectionList", "componentId": "props" }
      ],
      "center": [
        { "component": "AddForm", "componentId": "props", "router": { "path": "/props/add", "isExact": true } },
        { "component": "AddForm", "componentId": "component", "router": { "path": "/components/add", "isExact": true } },
        { "component": "ViewProps", "router": { "path": "/props/view/:id", "isExact": true } },
        { "component": "ViewComponent", "router": { "path": "/components/view/:id", "isExact": true } }
      ]
    }
  }
}
